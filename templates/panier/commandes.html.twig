{{ client }}
{% for commande in commandes %}
    <h3>Commande du {{ commande.dateCommande|date('d-m-y') }}</h3>
    <ul>
        {% for content in commande.contenus %}

            <li>
                {{ content.produit.designation }}
                nombre {{ content.quantite }}
            </li>

        {% endfor %}
    </ul>

{% endfor %}
